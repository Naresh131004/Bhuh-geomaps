	<script>
	var userProfile = JSON.parse(localStorage.getItem('userProfile'));

	if( (userProfile == null || userProfile.account_type == "free") && !iOSapp){
		//USER HAS FREE ACCOUNT SO SHOW AD BOX
		var adbox = document.getElementById("flc");
		adbox.classList.add("show");

		/*
		var mapcontainer = document.getElementById("map-container");
		mapcontainer.classList.add('free');
		var body = document.getElementsByTagName("body")[0];
		body.classList.add('free');
		*/

		/*
		//LOAD Google Ads SCRIPT ON .COM
		if( root != ".net" ){
			(adsbygoogle = window.adsbygoogle || []).push({});
		}
		*/
	}
	else{
		var mapcontainer = document.getElementById("map-container");
		mapcontainer.classList.remove('free');
		var body = document.getElementsByTagName("body")[0];
		body.classList.remove('free');
	}

	//USER HAS ADBLOCKER SO SHOW TEXT
	if( window.canRunAds === undefined ){
		if( (userProfile == null || userProfile.account_type == "free") && !iOSapp){
			var adbox = document.getElementById("flc-text");
			adbox.classList.add("show");
			var adbox = document.getElementById("flc-text-2");
			adbox.classList.add("show");
			/*
			var mapcontainer = document.getElementById("map-container");
			mapcontainer.classList.add('free');
			var body = document.getElementsByTagName("body")[0];
			body.classList.add('free');
			*/
		}
		else{
			var mapcontainer = document.getElementById("map-container");
			mapcontainer.classList.remove('free');
			var body = document.getElementsByTagName("body")[0];
			body.classList.remove('free');
		}
	}
    </script>


